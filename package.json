    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "graphql": "^16.6.0",
    "prettier": "^2.8.7",
    "prisma": "^4.15.0",
    "ts-migrate": "^0.1.35",
    "ts-node": "^10.9.1",
    "typegraphql-prisma": "^0.25.1",
    "typescript": "^5.0.4"
  }
